{
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "camelcase": [0],
    "quotes": [1, "single"],
    "curly": [0],
    "eqeqeq": [0],
    "dot-notation": [2, {"allowKeywords": false}],
    "no-underscore-dangle": [0],
    "consistent-return": [0],
    "no-use-before-define": [2, "nofunc"]
  },
  "globals": {
    "window": true,
    "document": true
  }
}